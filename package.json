{
  "name": "sdamgia",
  "homepage": "https://ilyasidorchik.github.io/sdamgia/",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@types/classnames": "2.2.10",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.0",
    "@types/pretty": "2.0.0",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-router-dom": "5.1.3",
    "@types/vfile-message": "2.0.0",
    "axios": "0.19.2",
    "gh-pages": "2.2.0",
    "node-sass": "4.13.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@bem-react/classname": "1.5.8",
    "@bem-react/classnames": "1.3.9",
    "@testing-library/react": "10.0.2",
    "classnames": "2.2.6",
    "harmony-reflect": "1.6.1",
    "husky": "4.2.3",
    "lint-staged": "10.1.2",
    "prettier": "2.0.2",
    "pretty": "2.0.0",
    "stylelint": "13.3.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-order": "4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:c": "CI=true react-scripts test --coverage --colors",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/index.ts",
      "!src/react-app-env.d.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty && CI=true yarn test --colors"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write --config ./.prettierrc "
    ],
    "!(_*).scss": [
      "stylelint --fix --config ./.stylelintrc"
    ]
  }
}
